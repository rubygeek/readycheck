<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p> 
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p> 
  <p>  
     <%= f.label :password, "Password" %>  
     <%= f.password_field :password %>  
  </p>  
  <p>  
     <%= f.label :password_confirmation, "Password confirmation" %>  
     <%= f.password_field :password_confirmation %>  
  </p>  
  <p>
    <%= f.label :access_level %><br />
    <%= f.text_field :access_level %>
  </p>
  <p>
    <%= f.label :gmt_offset %><br />
    <%= f.text_field :gmt_offset %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>